<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="166.0" prefWidth="300.0" styleClass="background" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.SaveEventCtrl">
   <children>
      <Text fx:id="saveEventTitle" fill="#413aff" layoutX="81.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Save event">
         <font>
            <Font size="29.0" />
         </font>
      </Text>
      <TextField fx:id="eventIdField" alignment="CENTER" layoutX="75.0" layoutY="83.0" promptText="Enter event id" />
      <Button fx:id="confirmButton" layoutX="81.0" layoutY="118.0" mnemonicParsing="false" onAction="#saveEvent" text="Confirm" />
      <Button fx:id="cancelButton" layoutX="167.0" layoutY="118.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
   <stylesheets>
      <URL value="@client/styles/Styles.css" />
      <URL value="@../styles/Styles.css" />
   </stylesheets>
</AnchorPane>
